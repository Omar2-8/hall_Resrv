@model Hall_Reservation.Models.Visa

@{
    ViewData["Title"] = "Create";
     Layout = "~/Views/Shared/Home.cshtml"; 
}



<h4>Visa</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="VisaName" class="control-label"></label>
                <input asp-for="VisaName" class="form-control" />
                <span asp-validation-for="VisaName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VisaNumber" class="control-label"></label>
                <input asp-for="VisaNumber" class="form-control" />
                <span asp-validation-for="VisaNumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VisaAmount" class="control-label"></label>
                <input asp-for="VisaAmount" class="form-control" />
                <span asp-validation-for="VisaAmount" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EndDate" class="control-label"></label>
                <input asp-for="EndDate" class="form-control" />
                <span asp-validation-for="EndDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CvcCvv" class="control-label"></label>
                <input asp-for="CvcCvv" class="form-control" />
                <span asp-validation-for="CvcCvv" class="text-danger"></span>
            </div>
             
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



 